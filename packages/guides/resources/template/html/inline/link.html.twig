{%- if node.url -%}
    <a href="{{- node.url -}}"
        {%- for key, value in attributes %} {{- key -}}="{{- value -}}"{% endfor -%}
        {%- if node.classes %} class="{{ node.classesString }}"{% endif -%}
    >
    {%- for child in node.children -%}
        {{- renderNode(child) -}}
    {%- endfor -%}
    </a>
{%- else -%}
    {%- for child in node.children -%}
        {{- renderNode(child) -}}
    {%- endfor -%}
{%- endif -%}

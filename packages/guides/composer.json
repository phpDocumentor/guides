{
    "name": "phpdocumentor/guides",
    "description": "Contains the core functionality from the phpDocumentor Guides.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://www.phpdoc.org",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "phpDocumentor\\Guides\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "phpDocumentor\\Guides\\": [
                "tests/unit/"
            ]
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "league/flysystem": "^1.0",
        "league/tactician": "^1.1",
        "league/uri": "^6.5",
        "phpdocumentor/flyfinder": "^1.1",
        "psr/event-dispatcher": "^1.0",
        "symfony/string": "^5.4 || ^6.3",
        "symfony/translation-contracts": "^3.3.0",
        "twig/twig": "~2.15",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "psr/log": "^1.0"
    }
}

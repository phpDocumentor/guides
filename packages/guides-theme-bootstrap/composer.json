{
    "name": "phpdocumentor/guides-theme-bootstrap",
    "description": "A theme for phpdocumentor/guides based on Bootstrap",
    "type": "project",
    "license": "MIT",
    "homepage": "https://www.phpdoc.org",
    "autoload": {
        "psr-4": {
            "phpDocumentor\\Guides\\ThemeBootstrap\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "monolog/monolog": "^2.9",
        "phpdocumentor/guides": "dev-main@dev",
        "phpdocumentor/guides-restructured-text": "dev-main@dev",
        "phpdocumentor/guides-symfony": "dev-main@dev",
        "symfony/config": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4"
    },
    "bin": [
        "bin/guides-theme-bootstrap"
    ]
}

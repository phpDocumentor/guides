{
    "name": "phpdocumentor/guides-theme-bootstrap",
    "description": "A theme for phpdocumentor/guides based on Bootstrap",
    "type": "library",
    "license": "MIT",
    "homepage": "https://www.phpdoc.org",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "phpDocumentor\\Guides\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "phpdocumentor/guides": "^1.0",
        "phpdocumentor/guides-restructured-text": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}

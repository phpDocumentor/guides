{
    "name": "phpdocumentor/guides",
    "type": "library",
    "license": "MIT",
    "homepage": "https://www.phpdoc.org",
    "description": "Core library to parse and render MarkupLanguages, focused on documentation",
    "autoload": {
        "psr-4": {
            "phpDocumentor\\Guides\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "phpDocumentor\\Guides\\": [
                "tests/unit/"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "7.4.1"
        }
    },
    "require": {
        "php": "^7.4||^8.0",
        "ext-mbstring": "*",
        "league/flysystem": "^1.0",
        "phpdocumentor/flyfinder": "^1.1",
        "twig/twig": "~3.3",
        "webmozart/assert": "^1.3",
        "league/tactician": "^1.1",
        "psr/event-dispatcher": "^1.0",
        "symfony/string": "^5.4",
        "symfony/translation-contracts": "^2.1",
        "league/uri": "^6.5",
        "league/uri-interfaces": "^2.3",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.22",
        "phpstan/phpstan": "^1.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^1.1"
    }
}
